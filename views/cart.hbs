<section>
            <div class="cart__container">
                <div class="cart__content">
                    <h2>My Cart</h2><span class="cart-header__text">({{cart.count}} items)</span>
                </div>
                 {{#each cart.items}}
                <div class="cart__content__item"  data-id="{{product.id}}">
                    <!-- <figure> -->
                        <img src="{{product.imageURL}}" alt="{{product.name}}" class="items">
                    <!-- </figure> -->
                    <div class="cart__content__items">
                        <h3>{{product.name}}</h3>
                        <div class="item__count">
                            <button type="button" class="plus__count" aria-label="plus" data-id="{{product.id}}">&#43;</button>
                            <span  class="cart__counter" data-id="{{product.id}}">{{count}}</span>
                            <button type="button" class="minus__count" aria-label="minus" data-id="{{product.id}}">&#45;</button>
                            <span class="multiply" aria-label="multiplication">&times;</span>
                            <span>Rs {{product.price}}</span>
                        </div>
                    </div>
                    <div class="total__price" aria-label="total price" data-id="{{product.id}}">Rs {{multiply product.price count}} </div>
                </div>
                  {{/each}}
                <div class="cart__content__price">
                    <img src="/static/images/lowest-price.png" alt="lowest price" class="lowest__price">
                    <span>You won't find it cheaper anywhere.</span>
                </div>
                
            </div>
            
        </section>
        <div class="cart__checkout__btn">
                <p>Promto code can be applied on payment page.</p>
                <a href="#" class="btn checkout__btn">
                <span>Proceed to checkout</span>
                <span>Rs. 187</span>
            </a>
        </div>

        <script src="js/cart.js" type="text/javascript"></script>
